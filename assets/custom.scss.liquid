.tag--item.tag--securitty--tag {
    width: max-content;
    max-width: 100%;
    position: relative;
    display: flex;
    justify-content: flex-start;
    align-items: flex-start;
    overflow: hidden;
    margin-top: 10px;
    &.final--sale__tag{
        .absolute--bg--tag {
            background-color: #f3d7d8;
        }
        .relative--tag--securitty a.open--popup--tag{
            span.name--securitty--item {
                color: #d33a3e;
            }
            span.icon--securitty--item path {
                fill: #d33a3e !important;
            }
        }
    }
    &.clearance--sale__tag{
        .absolute--bg--tag {
            background-color: #d7f3f0;
        }
        .relative--tag--securitty a.open--popup--tag{
            span.name--securitty--item {
                color: #26a4b7;
            }
            span.icon--securitty--item path {
                fill: #26a4b7 !important;
            }
        }
    }
    .absolute--bg--tag {
        position: absolute;
        z-index: 1;
        left: 0;
        right: 0;
        top: 0;
        height: 100%;
        width: 100%;
        max-width: 100%;
        background-color: #f3edd7;
        &:before {
            content: '';
            position: absolute;
            top: -15px;
            left: -15px;
            width: 30px;
            height: 30px;
            z-index: 2;
            background-color: #fff;
            transform: rotate(45deg);
        }
        &:after {
            content: '';
            position: absolute;
            bottom: -15px;
            left: -15px;
            width: 30px;
            height: 30px;
            z-index: 2;
            background-color: #fff;
            transform: rotate(45deg);
        }
    }
    
    .relative--tag--securitty {
        width: max-content;
        max-width: 100%;
        position: relative;
        z-index: 4;
        a.open--popup--tag {
            width: max-content;
            max-width: 100%;
            display: flex;
            justify-content: flex-start;
            align-items: center;
            padding-left: 30px;
            padding-right: 10px;
            padding-top: 10px;
            padding-bottom: 10px;
            &:hover{
                text-decoration: unset;
                span{
                    text-decoration: unset !important;
                }
            }
            span.name--securitty--item {
                color: #b78226;
                font-family: Jost;
                font-size: 16px;
                font-weight: 400;
            }
            
            span.icon--securitty--item {
                width: 15px;
                height: 15px;
                display: flex;
                justify-content: center;
                align-items: center;
                margin-left: 15px;
                svg {
                    width: 100%;
                    height: 100%;
                    max-width: 100%;
                    object-fit: contain;
                    object-position: center;
                }
                path {fill: #b78226 !important;}
            }
        }
    }
}
