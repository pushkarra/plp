        let parent = $(this).parent().parent().parent();
        if (parent.find('.agree').is(':checked')) {
          ire('trackConversion', 24719, {
              orderId: "{{ customer.id }}",
              customerId: "{{ customer.id }}",
              customerEmail: '{{ email_hash }}'
            },
            {
              verifySiteDefinitionMatch:true
            }
          );
        }
      });
    </script>
